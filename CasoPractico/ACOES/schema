CREATE TABLE PERSONAL_ASOCIACION (IDENTIFICADOR BIGINT NOT NULL, APELLIDOS VARCHAR(255), CARGO VARCHAR(255), DESCRIPCION VARCHAR(255), EXPERIENCIA VARCHAR(255), FECHA_ENTRADA_ACCOES DATE, FECHA_NACIMIENTO DATE, FOTO VARCHAR(255), NOMBRE VARCHAR(255), PRIMARY KEY (IDENTIFICADOR))
CREATE TABLE DATOS_ACADEMICOS (IDENTIFICADOR BIGINT NOT NULL, CENTRO VARCHAR(255), CURSO VARCHAR(255), NOTA_MEDIA BIGINT, PRIMARY KEY (IDENTIFICADOR))
CREATE TABLE INGRESOS_REGRESOS (CODIGO BIGINT NOT NULL, CANTIDAD BIGINT, DESCRIPCION VARCHAR(255), FECHA DATE, FECHA_PAGO DATE, TIPO VARCHAR(255), PRIMARY KEY (CODIGO))
CREATE TABLE BENEFICIARIO (CODIGO BIGINT NOT NULL, APELLIDOS VARCHAR(255), BECA VARCHAR(255), COMUNIDAD_PROCEDENCIA VARCHAR(255), COMUNIDAD_RESIDENCIA VARCHAR(255), ESTADO VARCHAR(255), FECHA_ENTRADA_ACOES DATE, FECHA_NACIMIENTO DATE, FECHA_SALIDA_ACOES DATE, FOTO VARCHAR(255), GRADO_CURSO VARCHAR(255), HABILITADO SMALLINT DEFAULT 0, NOMBRE VARCHAR(255), OBSERVACIONES VARCHAR(255), SEXO VARCHAR(255), TIPO_BENEFICIARIOS VARCHAR(255), PRIMARY KEY (CODIGO))
CREATE TABLE SOCIOS (ID_SOCIO BIGINT NOT NULL, APELLIDOS VARCHAR(255), CERTIFICADO VARCHAR(255), CODIGO_POSTAL BIGINT, DIRECCION VARCHAR(255), EMAIL VARCHAR(255), ESTADO VARCHAR(255), FECHA_ALTA DATE, FECHA_BAJA DATE, NIF BIGINT, NOMBRE VARCHAR(255), OBSERVACIONES VARCHAR(255), PROVINCIA VARCHAR(255), RELACION VARCHAR(255), SECTOR VARCHAR(255), TELEFONO_FIJO BIGINT, TELEFONO_MOVIL BIGINT, PRIMARY KEY (ID_SOCIO))
CREATE TABLE SEQUENCE (SEQ_NAME VARCHAR(50) NOT NULL, SEQ_COUNT DECIMAL(15), PRIMARY KEY (SEQ_NAME))
INSERT INTO SEQUENCE(SEQ_NAME, SEQ_COUNT) values ('SEQ_GEN', 0)
